    // UID for the DLL
    dll_uid = KCenRepSourcePlugInDllUid;
    // Declare array of interface info
    interfaces = 
        {
        INTERFACE_INFO
            {
            // UID of interface that is implemented
            interface_uid = KContextSourcePluginInterfaceUid;
            implementations = 
                {
                IMPLEMENTATION_INFO
                    {
                    implementation_uid = KCenRepSourcePlugInImplementationUid;
                    version_no         = 1;
                    display_name       = "CenRepSource";
                    default_data       = "";
                    opaque_data        = "";
                    }
                };
            }
        };
    }
