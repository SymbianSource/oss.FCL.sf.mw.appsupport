<script xmlns="http://www.nokia.com/ns/cep/script/1.0/"
xmlns:cep="http://www.nokia.com/ns/cep/1.0/">
    <if>
        <greaterOrEqual>
            <contextRef source='Sensor' type='Y.RawValue'/>
            <cep:int>151</cep:int>
        </greaterOrEqual>
        <actions>
            <!-- start calendar app -->
            <startApp appUid='10005901' viewUid='00000001' msgUid='00000000' msg='msg'/>
        </actions>
    </if>
</script>
