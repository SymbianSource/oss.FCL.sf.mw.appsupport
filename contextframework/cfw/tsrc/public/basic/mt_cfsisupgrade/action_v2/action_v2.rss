#include <ecom/RegistryInfo.rh>
#include <CFActionPluginConst.hrh>

#include "action_v2.hrh"

RESOURCE REGISTRY_INFO theInfo
	{
	// UID for the DLL
	dll_uid = KAction_V2DllUid;
	// Declare array of interface info
	interfaces = 
		{
		INTERFACE_INFO
			{
			// UID of interface that is implemented
			interface_uid = KActionPluginInterfaceUid;
			implementations = 
				{
				IMPLEMENTATION_INFO
					{
					implementation_uid = KAction_V2ImplUid;
					version_no         = 2;
					display_name       = "";
					default_data       = "";
					opaque_data        = "";
					}
				};
			}
		};
	}

